<Window x:Class="MyPractises.DotnetWindows.FileIOWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyPractises.DotnetWindows"
        mc:Ignorable="d"
        Title="FileIOWin" Height="300" Width="300">
    <StackPanel>
        <Button Content="MyPicture File Path" Click="Button_Click_1"></Button>
        <Button Content="Show Files in current directory" Click="Button_Click"></Button>
        <Button Content="Open FileDialog" Click="Button_Click_2"></Button>
        <Button Content="Get FileVersionInfo" Click="Button_Click_3"></Button>
        <Button Content="Get Video Duration" Click="Button_Click_4"></Button>
        <TextBox Name="tbShowFiles" Height="100" VerticalScrollBarVisibility="Auto"></TextBox>
    </StackPanel>
</Window>
