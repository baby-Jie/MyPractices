<Window x:Class="MyPractises.DotnetWindows.HttpWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyPractises.DotnetWindows"
        mc:Ignorable="d"
        Title="HttpWin" Height="300" Width="500">
    <StackPanel Background="AntiqueWhite">
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Url:" Margin="10,0"></TextBlock>
            <TextBox Text="http://192.168.1.40:8001/ptzimg/4" Width="400" Name="tbUrl"></TextBox>
        </StackPanel>
        <Button Content="DownloadFiles" Click="Button_Click"></Button>
        <Button Content="UploadFiles" Click="Button_Click_1"></Button>
        <Button Content="Post" Click="Button_Click_2"></Button>
        <TextBox Name="tbShowmsg"  TextWrapping="Wrap" Margin="10" Height="70"></TextBox>
    </StackPanel>
</Window>
